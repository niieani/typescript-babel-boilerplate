{
  "name": "typescript-babel-node-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for TS -> ES6 -> ES5 for modern node development",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "gulp build",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "moduleIds": false,
    "comments": false,
    "compact": false,
    "plugins": [
      "babel-plugin-transform-es2015-destructuring",
      "babel-plugin-transform-es2015-modules-commonjs",
      "babel-plugin-transform-es2015-parameters",
      "babel-plugin-transform-es2015-sticky-regex",
      "babel-plugin-transform-es2015-unicode-regex"
    ],
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/niieani/typescript-babel-sass-jspm-boilerplate.git"
  },
  "keywords": [
    "typescript",
    "es6",
    "ecmascript",
    "es7",
    "es2016",
    "babel"
  ],
  "author": "Bazyli Brz√≥ska <npm@invent.life>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/niieani/typescript-babel-sass-jspm-boilerplate/issues"
  },
  "homepage": "https://github.com/niieani/typescript-babel-sass-jspm-boilerplate#readme",
  "devDependencies": {
    "babel-plugin-transform-es2015-destructuring": "^6.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.0.0",
    "babel-plugin-transform-es2015-parameters": "^6.0.0",
    "babel-plugin-transform-es2015-sticky-regex": "^6.0.0",
    "babel-plugin-transform-es2015-unicode-regex": "^6.0.0",
    "conventional-changelog": "^0.5.1",
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.0",
    "gulp-bump": "^1.0.0",
    "gulp-changed": "^1.3.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-protractor": "^1.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^3.5.0",
    "gulp-typescript": "^2.9.2",
    "gulp-yuidoc": "^0.1.2",
    "object.assign": "^4.0.3",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.4",
    "vinyl-paths": "^2.0.0",
    "yargs": "^3.29.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.0.16"
  }
}
